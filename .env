# AniList auth endpoint.
VUE_APP_ANILIST_AUTH_URL="https://anilist.co/api/v2/oauth/authorize?response_type=token"

# AniList GraphQL endpoint.
VUE_APP_ANILIST_GRAPHQL_URL="https://graphql.anilist.co"

# AniList search endpoints.
VUE_APP_ANILIST_ANIME_URL="https://anilist.co/search/anime"
