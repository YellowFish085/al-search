VUE_APP_ANILIST_STUDIOS_URL="https://anilist.co/search/studios"
VUE_APP_ANILIST_CHARACTERS_URL="https://anilist.co/search/characters"
VUE_APP_ANILIST_STAFF_URL="https://anilist.co/search/staff"
